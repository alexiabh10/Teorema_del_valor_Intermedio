%Ejercicio 2: x^2-3*x-2, [1,4]

clc
clear
syms x;
fx= input("INGRESE LA FUNCION :")
funcion = inline (char(fx))
a= input("INGRESE EL VALOR DEL LIMITE INFERIOR a: "); 
b= input("INGRESE EL VALOR DEL LIMITE SUPERIOR b: ");
c= input ("INFRESE LA CONSTANTE C : ");
resultado1= b^2-3*b-2
resultado2= a^2-3*a-2
total= resultado1< c < resultado2  

fprintf( "No es verdad que 1 esta entre los intervalos de 1 a 4");

x = 1:0.2:4;
f=x;
plot(f,x);
grid on
hold on
plot(f,x )
